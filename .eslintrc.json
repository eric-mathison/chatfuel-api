{
    "extends": "airbnb-base",
    "plugins": ["mocha"],
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "only-multiline"]
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "mocha": true,
        "jquery": true
    },
    "globals": {
        "chai": true,
        "expect": true
    }
}
